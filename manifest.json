{
  "manifest_version": 2,
  "name": "Flower power",
  "version": "0.1",
  "content_scripts":[
    {
      "matches": [
      "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "flower.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ]
}
